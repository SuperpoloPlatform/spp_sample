<?xml version="1.0" encoding="UTF-8"?>
<library>
	<sounds>
		<!-- 这里添加音效
		<sound name="spider1" file="spider1.wav" />
		-->
	</sounds>
	<!-- import玩家的lib -->
	<plugins>
		<plugin name='null'>crystalspace.mesh.loader.null</plugin>
		<plugin name='genmesh'>crystalspace.mesh.loader.genmesh</plugin>
		<plugin name='genmeshfact'>crystalspace.mesh.loader.factory.genmesh</plugin>
		<plugin name="cal3d">crystalspace.mesh.loader.sprite.cal3d</plugin>
		<plugin name='cal3dfact'>crystalspace.mesh.loader.factory.sprite.cal3d</plugin>
	</plugins>
	<!-- 这里添加shader
	<shaders>
		<shader>
			<file>/shader/character/character_bump2.xml</file>
		</shader>
		<shader>
			<file>/shader/character/character2.xml</file>
		</shader>
	</shaders>
	-->
	<textures>
		<texture name="jian"><file>/art/models/nv/textures/jian.jpg</file></texture>
		<texture name="katongnv"><file>/art/models/nv/textures/katongnv.jpg</file></texture>
		<texture name="nv_juese"><file>/art/models/nv/textures/nv_juese.png</file></texture>
	</textures>
	<materials>
		<material name="nv_jian.jpg">
			<texture>jian</texture>
			<shader type="diffuse">light</shader>
			<shadervar name="light 0 diffuse" type="vector4">0.6, 0.8, 1, 1</shadervar>
		</material>
		<material name="nv_katongnv.jpg">
			<texture>katongnv</texture>
			<shader type="diffuse">light</shader>
			<shadervar name="light 0 diffuse" type="vector4">0.6, 0.8, 1, 1</shadervar>
		</material>
		<material name="nv_nv_juese.png">
			<texture>nv_juese</texture>
			<shader type="diffuse">light</shader>
			<shadervar name="light 0 diffuse" type="vector4">0.6, 0.8, 1, 1</shadervar>
		</material>
	</materials>
	<library>/art/models/nv/factories/genNv.xml</library>
</library>