<?xml version='1.0' encoding='UTF-8'?>
<library>
	<plugins>
		<plugin name='null'>crystalspace.mesh.loader.null</plugin>
		<plugin name='genmesh'>crystalspace.mesh.loader.genmesh</plugin>
		<plugin name='genmeshfact'>crystalspace.mesh.loader.factory.genmesh</plugin>
		<plugin name="cal3d">crystalspace.mesh.loader.sprite.cal3d</plugin>
		<plugin name="cal3dfact">crystalspace.mesh.loader.factory.sprite.cal3d</plugin>
	</plugins>
	<!-- 这里添加shader
	<shaders>
		<shader>
			<file>/shader/character/character_bump2.xml</file>
		</shader>
		<shader>
			<file>/shader/character/character2.xml</file>
		</shader>
	</shaders>
	-->
	<textures>
		<texture name="lian"><file>/art/models/nv2/textures/lian.jpg</file></texture>
		<texture name="yifu"><file>/art/models/nv2/textures/yifu.jpg</file></texture>
		<texture name="pifeng"><file>/art/models/nv2/textures/pifeng.jpg</file></texture>
	</textures>
	<materials>
		<material name="nv2_lian.jpg">
			<texture>lian</texture>
			<shader type="diffuse">light</shader>
			<shadervar name="light 0 diffuse" type="vector4">0.6, 0.8, 1, 1</shadervar>
		</material>
		<material name="nv2_yifu.jpg">
			<texture>yifu</texture>
			<shader type="diffuse">light</shader>
			<shadervar name="light 0 diffuse" type="vector4">0.6, 0.8, 1, 1</shadervar>
		</material>
		<material name="nv2_pifeng.jpg">
			<texture>pifeng</texture>
			<shader type="diffuse">light</shader>
			<shadervar name="light 0 diffuse" type="vector4">0.6, 0.8, 1, 1</shadervar>
		</material>
	</materials>
	<library>factories/genNv2.xml</library>
</library>
