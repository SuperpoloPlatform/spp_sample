		<StateImagery name="DisabledNoTitleNoFrame">
			<Layer>

			</Layer>
		</StateImagery>
	</WidgetLook>
	<!--
    ***************************************************
        interface/DefaultWindow
    ***************************************************
    -->
	<WidgetLook name="interface/DefaultWindow">
