	<WidgetLook name="Demo/ComboEditbox">
		<PropertyDefinition name="NormalTextColour" initialValue="FF000000" redrawOnWrite="true"/>
		<PropertyDefinition name="SelectedTextColour" initialValue="FFFFFFFF" redrawOnWrite="true"/>
		<PropertyDefinition name="ActiveSelectionColour" initialValue="FF000000" redrawOnWrite="true"/>
		<PropertyDefinition name="InactiveSelectionColour" initialValue="FF999999" redrawOnWrite="true"/>
