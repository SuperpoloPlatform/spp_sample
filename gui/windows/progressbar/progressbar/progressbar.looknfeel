	<WidgetLook name="Demo/Progressbar">
	<Property name="VerticalProgress" value="False"></Property>
	<Property name="ReversedProgress" value="False"></Property>
	<Property name="CurrentProgress" value="0.3"></Property>
		<NamedArea name="ProgressArea">
			<Area>
				<Dim type="LeftEdge">
				     <AbsoluteDim value="0"></AbsoluteDim>
				</Dim>
				<Dim type="TopEdge">
					   <AbsoluteDim value="0"></AbsoluteDim>
				</Dim>
				<Dim type="Width" ><UnifiedDim scale="1" type="Width" /></Dim>
                 <Dim type="Height" ><UnifiedDim scale="1" type="Height" /></Dim>
			</Area>
		</NamedArea>
		<ImagerySection name="frame">
		<FrameComponent>
			<Area>
				<Dim type="LeftEdge">
					<AbsoluteDim value="0"/>
				</Dim>
				<Dim type="TopEdge">
				<AbsoluteDim value="0"/>
				</Dim>
				<Dim type="Width">
					<UnifiedDim scale="1" type="Width"/>
				</Dim>
				<Dim type="Height">
					<UnifiedDim scale="1" type="Height"></UnifiedDim>
				</Dim>
			</Area>
			<Image type="LeftEdge" imageset="ice" image="ProgressBarLeft"/>
			<Image type="RightEdge" imageset="ice" image="ProgressBarRight"/>
			<Image type="Background" imageset="ice" image="ProgressBarMiddle"/>
			</FrameComponent>
		</ImagerySection>
	    <ImagerySection name="tiled" >
           <ImageryComponent>
                <Area>
                    <Dim type="LeftEdge" ><AbsoluteDim value="0" /></Dim>
                    <Dim type="TopEdge" ><AbsoluteDim value="0" /></Dim>
                    <Dim type="Width" ><UnifiedDim scale="1" type="Width" /></Dim>
                    <Dim type="Height" ><UnifiedDim scale="1" type="Height" /></Dim>
                </Area>
                <Image imageset="ice" image="ProgressBarLitSegment" />
                <VertFormat type="Stretched" />
                <HorzFormat type="Tiled" />
            </ImageryComponent>
        </ImagerySection>
		<StateImagery name="Enabled">
		<Layer><Section section="frame"/></Layer>
		</StateImagery>
	    <StateImagery name="Diabled">
	    </StateImagery>
		<StateImagery name="EnabledProgress">
		<Layer>
		  <Section section="tiled"></Section>
		</Layer>
		</StateImagery>
		<StateImagery name="DisabledProgress">
		 <Layer>
		   <Section section="tile">
		   <Colours topLeft="FF7F7F7F" topRight="FF7F7F7F" bottomLeft="FF7F7F7F" bottomRight="FF7F7F7F" />
		   </Section>
		 </Layer>
		</StateImagery>
</WidgetLook>
